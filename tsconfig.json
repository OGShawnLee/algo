{
  "compilerOptions": {
    "target": "ESNext",
    "types": ["vitest/globals", "vitest/importMeta"],
    "baseUrl": "src",
    "module": "ESNext",
    "paths": {
      "@data": ["data"],
      "@list": ["list"],
      "@list/*": ["list/*"],
      "@string": ["string"],
      "@string/*": ["string/*"],
      "@utils": ["utils"]
    },
    "moduleResolution": "node"
  }
}
